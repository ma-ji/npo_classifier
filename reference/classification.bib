
@article{FyallNTEECodesOpportunities2018,
  title = {Beyond {{NTEE Codes}}: {{Opportunities}} to {{Understand Nonprofit Activity Through Mission Statement Content Coding}}},
  volume = {47},
  issn = {0899-7640, 1552-7395},
  shorttitle = {Beyond {{NTEE Codes}}},
  doi = {10.1177/0899764018768019},
  abstract = {There are profound differences within the nonprofit sector, and research benefits from the ability to group nonprofits by substantive focus. Researchers typically rely on the National Taxonomy of Exempt Entities (NTEE) codes to categorize nonprofits, but we argue that mission statement text offers a better information source for nonprofit researchers to create categories of organizations. Harnessing advances in data availability and machine-reading technology, this article introduces a new method whereby mission statement analysis drives research and analysis of ``like'' organizations. Using an automated dictionary method to analyze mission statements, we draw a sample of housing and shelter nonprofits in Washington State. Compared with the corresponding sample based on NTEE classification, our results find roughly double the number of housing and shelter nonprofits based on their mission statements. Our method also proves more accurate than NTEE codes when applied to a sub-sample of nonprofits known to provide shelter for the homeless.},
  language = {en},
  number = {4},
  journal = {Nonprofit and Voluntary Sector Quarterly},
  author = {Fyall, Rachel and Moore, M. Kathleen and Gugerty, Mary Kay},
  month = aug,
  year = {2018},
  pages = {677-701},
  file = {C:\\Users\\jm92629\\Zotero\\storage\\TKVFVPRW\\Fyall et al. - 2018 - Beyond NTEE Codes Opportunities to Understand Non.pdf},
  note = {00000}
}

